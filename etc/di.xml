<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">
    <type name="Magento\PageBuilder\Model\WidgetInitializerConfig">
        <arguments>
            <argument name="config" xsi:type="array">
                <item name="faq" xsi:type="array">
                    <item name="default" xsi:type="array">
                        <item name="component" xsi:type="string">Liftmode_PageBuilderFaq/js/content-type/faq/appearance/default/widget</item>
                        <!--optional if you want load widget per appearance-->
                        <item name="appearance" xsi:type="string">default</item>
                    </item>
                </item>
            </argument>
        </arguments>
    </type>
</config>
